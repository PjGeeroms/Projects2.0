<div class="container">
    <div class="grid my-1 my-6@lg">
        <div class="col-12 col-5@lg">
            <div class="box--stack is-yellow px-3 pb-3 pt-4" data-aos="fade-up">
                <h3 class="h4">{{ block.ctaTitle }}</h3>
                <a class="btn btn--primary btn--fw" href="{{ block.ctaUrl }}">{{ block.ctaAction }}</a>
            </div>
        </div>
        <div class="col-12 col-os-7-13@lg">
            <div class="box--stack is-y-center is-white py-2" data-aos="fade-up" data-aos-delay="300">
                <p>{{ block.ctaDescription }}</p>
            </div>
        </div>
    </div>
</div>